		"concurrently": "^8.2.1",
		"near-social-local-viewer": "^2.1.3",
		"rome": "^12.1.3",
		"typescript": "^5.0.2",
		"unocss": "^0.55.0",
		"unocss-preset-theme": "^0.9.1",
